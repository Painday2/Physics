<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_mod_phys/characters/giant_bo/giant_bo" orientation_object="root_point"/>
	<sequence_manager file="units/pd2_mod_phys/characters/giant_bo/giant_bo"/>

	<animation_def name="anims/units/giant_bo/giant_bo"/>

	<decal_surfaces default_material="flesh"/>

	<bodies>
		<body name="Head" enabled="true" template="animated">
			<object name="Neck" />
			<object name="c_convex_Head" collision_type="convex"/> 
		</body>

		<body name="Hips" enabled="true" template="animated">
			<object name="Hips" />
			<object name="c_convex_Hips" collision_type="convex"/> 
		</body>

		<body name="Spine" enabled="true" template="animated">
			<object name="Spine" />
			<object name="c_convex_Spine" collision_type="convex"/> 
		</body>

		<body name="Spine2" enabled="true" template="animated">
			<object name="Spine2" />
			<object name="c_convex_Spine2" collision_type="convex"/> 
		</body>

		<body name="RightForeArm" enabled="true" template="animated">
			<object name="RightForeArm" />
			<object name="c_convex_RightForeArm" collision_type="convex"/> 
		</body>

		<body name="RightArm" enabled="true" template="animated">
			<object name="RightArm" />
			<object name="c_convex_RightArm" collision_type="convex"/> 
		</body>

		<body name="LeftArm" enabled="true" template="animated">
			<object name="LeftArm" />
			<object name="c_convex_LeftArm" collision_type="convex"/> 
		</body>

		<body name="LeftForeArm" enabled="true" template="animated">
			<object name="LeftForeArm" />
			<object name="c_convex_LeftForeArm" collision_type="convex"/> 
		</body>

		<body name="RightUpLeg" enabled="true" template="animated">
			<object name="RightUpLeg" />
			<object name="c_capsule_RightUpLeg" collision_type="capsule"/> 
		</body>

		<body name="RightLeg" enabled="true" template="animated">
			<object name="RightLeg" />
			<object name="c_capsule_RightLeg" collision_type="capsule"/> 
		</body>

		<body name="LeftUpLeg" enabled="true" template="animated">
			<object name="LeftUpLeg" />
			<object name="c_capsule_LeftUpLeg" collision_type="capsule"/> 
		</body>

		<body name="LeftLeg" enabled="true" template="animated">
			<object name="LeftLeg" />
			<object name="c_capsule_LeftLeg" collision_type="capsule"/> 
		</body>
	</bodies>

	<graphics>
		<graphic_group name="character" enabled="true" culling_object="co_culling">
			<object name="g_body" enabled="true"/>
			<object name="g_head" enabled="true"/>

			<object name="g_shield" enabled="false"/>

			<object name="s_body" enabled="true" shadow_caster="true"/>
		</graphic_group>
	</graphics>
</dynamic_object>