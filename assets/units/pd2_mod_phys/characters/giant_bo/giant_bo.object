		<object enabled="true" name="c_capsule_RightArm"/>
		<object enabled="true" name="c_capsule_LeftArm"/>
		<object enabled="true" name="c_capsule_RightForeArm"/>
		<object enabled="true" name="c_capsule_LeftForeArm"/>
		<object enabled="true" name="c_capsule_RightLeg"/>
		<object enabled="true" name="c_capsule_LeftLeg"/> -->
